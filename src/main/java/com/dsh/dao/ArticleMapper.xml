<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper  namespace="com.dsh.dao.ArticleMapper">
    <select id="getArticle" resultType="com.dsh.entity.Article">
        select id,user_name,title,content from article where id= #{id}
    </select>
</mapper>